{
	"plugins": [
		"prettier-plugin-svelte"
	],
	"arrowParens": "always",
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"tabWidth": 2,
	"printWidth": 100,
	"pluginSearchDirs": [
		"."
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}